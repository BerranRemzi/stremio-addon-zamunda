# Server Configuration
PORT=7000

# Tracker Configuration
ZAMUNDA_NET=false
ZAMUNDA_CH=false
ZAMUNDA_SE=false
ARENABG_COM=false
ZAMUNDA_RIP=true

# Zamunda.net Credentials
ZAMUNDA_NET_USERNAME=your_username
ZAMUNDA_NET_PASSWORD=your_password

# Zamunda.ch Credentials
ZAMUNDA_CH_DOMAIN=zamunda.ch
ZAMUNDA_CH_USERNAME=your_username
ZAMUNDA_CH_PASSWORD=your_password

# Zamunda.se Credentials
ZAMUNDA_SE_USERNAME=your_username
ZAMUNDA_SE_PASSWORD=your_password

# ArenaBG Credentials
ARENABG_USERNAME=your_username
ARENABG_PASSWORD=your_password

# Zamunda.rip (No credentials required - public tracker)
# Zamunda.rip API limit (default: 50, max results per search)
ZAMUNDA_RIP_LIMIT=50

# OMDb API Key (Optional - improves title detection)
OMDB_API_KEY=your_omdb_api_key

# Log Request URL (Optional - logs movie searches with GET request)
# Format: https://example.com/log?name={movieTitle}&id={imdbId}&tracker={trackerName}&token={ZAMUNDA_CH_PASSWORD}
LOG_REQUEST_URL=https://example.com/log