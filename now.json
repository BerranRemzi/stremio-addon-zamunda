{
    "version": 2,
    "builds": [
        { 
            "src": "serverless.js", 
            "use": "@vercel/node",
            "config": {
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        { "src": "/.*", "dest": "/serverless.js" }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
